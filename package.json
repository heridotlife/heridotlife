{
  "name": "astro-heridotlife",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "dev:wrangler": "./setup_db.sh && pnpm build && wrangler dev --persist-to=.wrangler/state --live-reload",
    "dev:wrangler:skip-build": "./setup_db.sh && wrangler dev --persist-to=.wrangler/state --live-reload",
    "start": "astro dev",
    "build": "astro build",
    "preview": "wrangler dev",
    "deploy": "wrangler deploy",
    "astro": "astro",
    "db:migrate": "wrangler d1 execute D1_db --file=./schema.sql",
    "db:migrate:local": "wrangler d1 execute D1_db --local --file=./schema.sql",
    "db:import": "wrangler d1 execute D1_db --local --file=./import_remote_data.sql",
    "db:setup": "pnpm db:migrate:local && pnpm db:import",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "eslint --fix . && prettier --write .",
    "type-check": "astro check"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.6.10",
    "@astrojs/react": "^4.4.0",
    "@astrojs/tailwind": "^6.0.2",
    "astro": "^5.14.3",
    "clsx": "^2.1.1",
    "jose": "^6.1.0",
    "lucide-react": "^0.545.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.9.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250110.0",
    "@eslint/js": "^9.37.0",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "eslint": "^9.37.0",
    "eslint-plugin-astro": "^1.3.1",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "typescript-eslint": "^8.46.0",
    "wrangler": "^4.42.2"
  }
}
