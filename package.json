{
  "name": "astro-heridotlife",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "dev:d1": "./setup_db.sh && pnpm build && wrangler pages dev dist --d1=D1_db --persist-to=.wrangler/state --live-reload",
    "dev:d1:skip-build": "./setup_db.sh && wrangler pages dev dist --d1=D1_db --persist-to=.wrangler/state --live-reload",
    "start": "astro dev",
    "build": "astro build",
    "preview": "wrangler pages dev dist --d1=D1_db",
    "astro": "astro",
    "db:migrate": "wrangler d1 execute D1_db --file=./schema.sql",
    "db:migrate:local": "wrangler d1 execute D1_db --local --file=./schema.sql",
    "db:import": "wrangler d1 execute D1_db --local --file=./import_remote_data.sql",
    "db:setup": "pnpm db:migrate:local && pnpm db:import"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^10.4.2",
    "@astrojs/react": "^4.4.0",
    "@astrojs/tailwind": "^5.1.0",
    "@vercel/og": "^0.8.5",
    "astro": "^4.5.12",
    "clsx": "^2.1.1",
    "jose": "^6.1.0",
    "lucide-react": "^0.545.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.9.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250110.0",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "wrangler": "^4.42.2"
  }
}
